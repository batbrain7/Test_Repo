<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mohitkumar.simpledatabase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".util.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".util.addata"
                    android:label=".util.addata"
            >
            <intent-filter>
                <action android:name="com.example.mohitkumar.introduction.collegedet"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".util.Datalist" />
        <activity android:name=".util.searchactivity" />
        <activity android:name=".util.deleteactivity" />
        <activity android:name=".util.update"></activity>
    </application>

</manifest>